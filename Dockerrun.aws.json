{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "auticparkic-api",
      "image": "bogdanjovanovic/auticparkic-api:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 10000
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "test"
        },
        {
          "name": "SPRING_DATASOURCE_URL",
          "value": "jdbc:postgresql://TODO:5432/autic_parkic"
        }
      ]
    }
  ]
}
